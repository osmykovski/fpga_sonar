# Definitional proc to organize widgets for parameters.
proc init_gui { IPINST } {
  ipgui::add_param $IPINST -name "Component_Name"
  #Adding Page
  set Page_0 [ipgui::add_page $IPINST -name "Page 0"]
  set half_period [ipgui::add_param $IPINST -name "half_period" -parent ${Page_0}]
  set_property tooltip {The length of the half-cycle of the sound wave in system cycles} ${half_period}


}

